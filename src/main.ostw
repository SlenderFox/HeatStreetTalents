			StartForcingDummyBotName(EventPlayer(), "Heavy Trooper");
			combat_pathfinding_type = 8;
			ab_var_3 = false;
			is_ranged = false;
			can_dodge = false;
			Damage_Dealt = 0.45;
			health_percent = 0.417;
		}
		else if (real_tier == 3)
		{
			StartForcingDummyBotName(EventPlayer(), "Grenadier");
			combat_pathfinding_type = 4;
			ab_var_3 = false;
			is_ranged = true;
			can_dodge = true;
			Damage_Dealt = 0.55;
			health_percent = 0.5;
		}
